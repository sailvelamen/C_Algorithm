#include <stdio.h>

int main()
{
    int i,j;

    for(i = 0; i < 5; i++)
    {
        for(j = 0; j <= 3 - i; j++)
        {
            printf(" ");
        }

        for(j = 0; j <= i * 2; j++)
        {
            printf("%c",'*');
        }

        printf("\n");
    }
    
    for(i = 0; i < 5; i++)
    {
        for(j = 0; j <= i; j++)
        {
            printf(" ");
        }

        for(j = 0; j < 7 - (i * 2); j++)
        {
            printf("*");
        }

        printf("\n");
    }

    return 0;
}
